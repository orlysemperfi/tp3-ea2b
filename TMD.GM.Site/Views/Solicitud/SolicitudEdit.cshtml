@model TMD.GM.Site.Models.SolicitudModel
<fieldset>
N° Solicitud:
@Html.TextBoxFor(m=> m.solicitudBE.NUMERO_SOLICITUD, new { @class = "txtForm", @readonly = "readonly" })
</fieldset>
<fieldset>
Fecha
@Html.TextBox("txtFechaSol", String.Format("{0:dd/MM/yyyy}", DateTime.Now.Date), new { @class = "txtFormFecha" })
Tipo de solicitud:
@Html.DropDownList("ddlTipoSolicitud",null, new { @class = "ddlForm" })
Documento Referencia:
@Html.TextBox("txtNumDocuRefe", "", new { @class = "txtForm" })
</fieldset>
<fieldset>
Equipo:
@Html.TextBox("txtCodigoEquipo", "", new { @class = "txtForm" })
@Html.TextBox("txtNombreEquipo", "", new { @class = "txtForm", style = "width: 200px;" })
<input id="btnBuscarEquipo" type="button" value="..." />
Área de Ubicación:
@Html.TextBox("txtAreaUbi", "", new { @class = "txtForm", @readonly = "readonly" })
</fieldset>
<fieldset>
Plan mantenimiento:
@Html.DropDownList("ddlPlanMante",null, new { @class = "ddlForm" })
Desde
@Html.TextBox("txtFechaIni", String.Format("{0:dd/MM/yyyy}", DateTime.Now.Date), new { @class = "txtFormFecha" })
Hasta
@Html.TextBox("txtFechaFin", String.Format("{0:dd/MM/yyyy}", DateTime.Now.Date), new { @class = "txtFormFecha" })
<input id="btnGenerar" type="button" value="Generar Cronograma" />
</fieldset>

<fieldset>
<input id="btnNuevo" type="button" value="Nueva Actividad" />
<input id="btnModificar" type="button" value="Modificar Actividad" />
<input id="btnEliminar" type="button" value="Eliminar Actividad" />
<input id="btnVerCalendario" type="button" value="Ver Calendario de Cronograma" />
Estado de solicitud:
@Html.DropDownList("ddlEstadoSolicitud", null, new { @class = "ddlForm", @disabled = "disabled" })
</fieldset>
<fieldset>
<input id="btnGrabar" type="button" value="Grabar Solicitud" />
<input id="btnRetornar" type="button" value="Retornar" onclick="CerrarModalSolicitudNueva()" />
</fieldset>
